    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "material-ui": "1.0.0-beta.45",
    "moment": "2.22.1",
    "node-sass-chokidar": "1.3.0",
    "npm-run-all": "4.1.2",
    "perfect-scrollbar": "^1.4.0",
    "prop-types": "15.6.2",
    "react": "16.3.1",
    "react-animate-on-scroll": "2.1.4",
    "react-chartist": "0.13.1",
    "react-datetime": "2.14.0",
    "react-dom": "16.3.1",
    "react-facebook-sdk": "^1.1.1",
